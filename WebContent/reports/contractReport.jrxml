			<staticText>
				<reportElement x="221" y="64" width="331" height="20" uuid="1b0da0b2-0968-4f51-b986-58df3911487d"/>
				<textElement textAlignment="Right" verticalAlignment="Top">
					<font fontName="Arial" size="13"/>
				</textElement>
				<text><![CDATA[الدفع قبل  البدء به.]]></text>
			</staticText>
